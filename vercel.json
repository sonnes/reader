{
  "buildCommand": "pnpm build",
  "outputDirectory": "dist/client",
  "rewrites": [{ "source": "/(.*)", "destination": "/_shell.html" }],
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=31536000, immutable" }
      ]
    }
  ]
}
